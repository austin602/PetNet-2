<h2>Blog Post List</h2>


<a class="btn btn-primary" href="/blog/admin/create">Create Post</a>


{{#each data.blogList}}
<div class="row">
    <div class="col-md-6">
        <div class="panel-body">
            <div>
                <strong>name: </strong> <a href="/blog/{{ id }}">{{ name }}</a>
            </div>
            <div>
                <strong>author: </strong>{{ author }}
            </div>
            <div>
                <strong>description: </strong>{{description}}
            </div>
        </div>
    </div>
</div>
{{/each}}
